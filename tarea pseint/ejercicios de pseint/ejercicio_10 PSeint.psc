Proceso ejercicio_10
	//Definir
	definir alto,ancho,area,perimetro Como Real;
	//operacion de entrada
	Escribir "digite la altura del rectangulo:";
	Leer alto;
	Escribir "digite el ancho del rectangulo";
	Leer ancho;
	//Proceso 
	area=alto*ancho;
	perimetro=alto*2+ancho*2;
	//operacion de salida
	Escribir "el area de ese rectangulo es: ",area;
	Escribir "el perimetro es: ",perimetro;
	
FinProceso
