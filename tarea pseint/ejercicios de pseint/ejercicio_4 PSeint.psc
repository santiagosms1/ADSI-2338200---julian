Proceso ejercicio_4
	Escribir "algoritmo que da el doble de la suma de dos digitos +10";
	//definicion
	Definir A,B,suma Como Entero;
	//proceso de entrada
	Escribir "porfavor digite el primer numero";
	leer A;
	Escribir "porfavor digite el segundo numero";
	leer B;
	//Proceso 
	suma=(A+B)*2+10;
